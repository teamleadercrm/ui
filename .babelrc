{
  "presets": ["@babel/preset-env", "@babel/preset-react", "@babel/preset-typescript"],
  "plugins": ["@babel/plugin-transform-runtime"],
  "env": {
    "cjs": {
      "plugins": ["transform-react-remove-prop-types"],
      "ignore": ["**/*.stories.js", "./src/static/**/*"]
    },
    "es": {
      "plugins": ["transform-react-remove-prop-types"],
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false
          }
        ]
      ],
      "ignore": ["**/*.stories.js", "./src/static/**/*"]
    }
  }
}
