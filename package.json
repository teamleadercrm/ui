  "homepage": "https://github.com/teamleadercrm/ui#readme",
  "keywords": [
    "teamleader",
    "ui"
  ],
  "license": "MIT",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teamleadercrm/ui.git"
  },
  "sideEffects": [
    "cjs/**/*.css",
    "es/**/*.css"
  ],
  "browserslist": ">0.5%, ie 11, not op_mini all",
  "scripts": {
    "build:css": "postcss ./src/**/*.css --base src --dir ./es && cp -R ./es ./cjs",
    "build:js": "npm run build:es; npm run build:cjs",
    "build:cjs": "NODE_ENV=cjs babel src --out-dir cjs",
    "build:es": "NODE_ENV=es babel src --out-dir es",
    "build": "rimraf es && rimraf cjs && npm run build:css && npm run build:js",
    "compile": "build-storybook -o dist",
    "lint:clean": "npm run lint && rimraf dist",
    "deploy:prod": "npm run lint:clean && NODE_ENV=production npm run compile",
    "lint": "eslint components stories",
    "lint:fix": "npm run lint -- --fix",
    "precommit": "pretty-quick --staged",
    "prettier": "prettier --write '**/*.{js,json,md,css}'",
    "prepare": "npm run build",
    "start": "PORT=${PORT:-3000}; start-storybook --port $PORT"
  }
}
